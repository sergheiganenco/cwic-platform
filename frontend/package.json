    "@xyflow/react": "^12.8.6",
    "clsx": "2.1.1",
    "dagre": "^0.8.5",
    "date-fns": "2.30.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "0.541.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.45.4",
    "react-markdown": "10.1.0",
    "react-redux": "8.1.2",
    "react-router-dom": "6.15.0",
