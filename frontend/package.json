    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "postcss": "^8.4.47",
    "prettier": "3.0.0",
    "tailwindcss": "3.3.0",
    "typescript": "5.9.2",
    "vite": "^5.4.11",
    "vitest": "0.34.6"
